<div class="page-header">
  <h3><%= current_user.username%></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Caption</th>
        <th>Image</th>
        <th>Actions</th>
      </tr>
      <% @mylikes.each do |photo| %>
      <tr>
        <td><%= photo.caption %></td>
        <td><img src="<%= photo.image %>" width="50"></td>
        <td>
          <a href="/photos/<%= photo.id %>" class="btn btn-primary">Show</a>
        </td>
    </tr>
    <% end  %>
  </table>
</div>
</div>
